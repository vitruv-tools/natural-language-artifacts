transformation networkToGraph for network to graph

import "http://vitruv.tools/methodologisttemplate/network"
import "http://vitruv.tools/methodologisttemplate/graph"

reaction ComponentRenamed {
	precondition change replace network.Component.name from c old value oldName new value newName
	call renameEntity(c, newName)
}

routine renameEntity(network.Component comp, String name) {
	update {
		graph.Entity entity = getCorrespondence(comp, graph.Entity)
		entity.name = name
	}
}