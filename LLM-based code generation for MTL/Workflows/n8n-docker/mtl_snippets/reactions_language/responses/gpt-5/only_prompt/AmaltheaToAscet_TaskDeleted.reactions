metamodel "http://vitruv.tools/methodologisttemplate/amalthea" as amalthea
metamodel "http://vitruv.tools/methodologisttemplate/ascet" as ascet
transformation amaltheaToAscet {
reaction TaskDeleted {
after delete amalthea::Task deletedTask
{
call deleteTask(deletedTask)
}
}
routine deleteTask(deletedTask: amalthea::Task) {
val tgt: ascet::Task = deletedTask.getOneCorrespondingElement(ascet::Task)
if (tgt != null) {
delete tgt
removeCorrespondence(deletedTask, tgt)
}
}
}